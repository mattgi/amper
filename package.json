{
  "name": "amper",
  "version": "1.0.0",
  "description": "amp builder",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .cache",
    "build": "npm run clean && npm run lambda-build && NODE_ENV=production node fuse dist",
    "lambda-serve": "netlify-lambda serve lambda/src -c lambda/webpack.lambda.js",
    "lambda-build": "netlify-lambda build lambda/src -c lambda/webpack.lambda.js",
    "local": "npm run clean && concurrently --kill-others \"npm run lambda-serve\" \"node fuse\"",
    "dev": "npm run clean && node fuse",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.ts,.tsx src lambda/src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|html|css|less|scss|ts|tsx|md|mdx|graphql|vue|py|java|php)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "public": true,
  "contributors": [
    {
      "name": "Christoffer RÃ¸dvik",
      "email": "snapgravy@gmail.com"
    },
    {
      "name": "Matt Ginty",
      "email": "matt.ginty@gmail.com"
    }
  ],
  "dependencies": {
    "@octokit/rest": "^16.28.7",
    "axios": "0.21.1",
    "crypto-js": "3.1.9-1",
    "debug": "4.1.1",
    "enter-view": "2.0.0",
    "eventemitter3": "4.0.0",
    "fastdom": "1.0.9",
    "gsap": "3.6.0",
    "history": "4.9.0",
    "intersection-observer": "0.7.0",
    "mongodb": "3.3.1",
    "netlify-lambda": "1.4.13",
    "object-path": "0.11.5",
    "polished": "3.4.1",
    "qrcode": "1.4.1",
    "quill": "1.3.7",
    "react": "16.9.0",
    "react-beautiful-dnd": "11.0.5",
    "react-color": "2.17.3",
    "react-compound-slider": "2.2.0",
    "react-dom": "16.9.0",
    "react-dropzone": "10.1.7",
    "rndm": "1.2.0",
    "scrollama": "2.0.1",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/aws-lambda": "8.10.27",
    "@types/axios": "0.14.0",
    "@types/debug": "4.1.5",
    "@types/gsap": "1.20.2",
    "@types/history": "4.7.2",
    "@types/mongodb": "3.3.0",
    "@types/node": "12.7.1",
    "@types/object-path": "0.11.0",
    "@types/react": "16.9.1",
    "@types/react-beautiful-dnd": "11.0.3",
    "@types/react-dom": "16.8.5",
    "@types/react-dropzone": "5.1.0",
    "@types/styled-components": "4.1.18",
    "concurrently": "4.1.1",
    "dotenv": "8.0.0",
    "fuse-box": "3.7.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "tslib": "1.10.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "webpack-node-externals": "1.7.2"
  }
